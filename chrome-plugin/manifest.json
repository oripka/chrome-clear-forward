{
  "manifest_version": 2,

  "name": "Chrome Pwned Extension",
  "description": "This extension monitors network traffic",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "background.html"
  },
  "permissions": [
    "webRequest", "webRequestBlocking",
    "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
